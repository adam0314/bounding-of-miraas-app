config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="bounding-of-miraas-app"
run/main_scene="res://scenes/bounding-of-miraas-app.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://scripts/Global.gd"

[display]

window/size/width=480
